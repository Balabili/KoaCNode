<!doctype html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <title>CNode:Node.js专业中文社区</title>
  <link rel="stylesheet" href="/stylesheets/common.css">
  <link rel="stylesheet" href="/stylesheets/style.css">
</head>

<body>
  <%- include('component/header.ejs') -%>
  <div id="main">
    <div id="sidebar">
      <% if(isLogon) { %>
        <%- include('component/sidebar_userInfo.ejs') -%>
        <%- include('component/sidebar_addTopic.ejs') -%>
      <% } else { %>
        <%- include('component/sidebar_logon.ejs') -%>
      <% } %>
      <%- include('component/sidebar_ad.ejs') -%>
      <%- include('component/sidebar_noReplyTopic.ejs') -%>
      <%- include('component/sidebar_point.ejs') -%>
      <%- include('component/sidebar_community.ejs') -%>
      <%- include('component/sidebar_erweima.ejs') -%>
    </div>
    <div id="container">
      <%- include('component/topicList.ejs') -%>
    </div>
    <div style="clear:both;"></div>
  </div>
  <div>
    <input type="hidden" name="userdata" id="userdata" value="<%= userData %>">
    <input type="hidden" name="islogon" id="islogon" value="<%= isLogon %>">
  </div>
  <%- include('component/footer.ejs') -%>
  <script src="/javascripts/common/jquery-3.2.1.min.js"></script>
  <script src="/index.bundle.js"></script>
</body>

</html>